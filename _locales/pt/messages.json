    "message": "Conteúdo do arquivo inválido. Faltando campos obrigatórios."
  },
  "errorFileRead": {
    "message": "Falha ao ler o arquivo: $message$",
    "placeholders": { "message": { "content": "$1" } }
  },
  "errorExportFailed": {
    "message": "Falha na exportação: $message$",
