  },
  "linkHelp": {
    "message": "도움말 / GitHub (오픈소스)"
  },
  "statusExporting": {
    "message": "내보내는 중..."
  },
  "statusImporting": {
    "message": "가져오는 중..."
  },
  "statusExportSuccess": {
    "message": "내보내기 성공!"
  },
  "statusImportSuccess": {
    "message": "$successes$개의 쿠키를 가져왔습니다! Bing 페이지를 새로고침하세요.",
    "placeholders": {
      "successes": { "content": "$1" }
    }
  },
  "statusImportPartial": {
    "message": "가져오기: $successes$개 성공, $failures$개 실패.",
    "placeholders": {
      "successes": { "content": "$1" },
      "failures": { "content": "$2" }
    }
  },
  "errorNoCookies": {
    "message": "오류: 관련 쿠키를 찾을 수 없습니다."
  },
  "errorFileFormatArray": {
    "message": "잘못된 파일 형식입니다. 배열이 아닙니다."
  },
  "errorFileEmpty": {
    "message": "파일 내용이 비어 있습니다."
  },
  "errorFileInvalid": {
    "message": "잘못된 파일 내용입니다. 필수 필드가 누락되었습니다."
  },
  "errorFileRead": {
    "message": "파일 읽기 실패: $message$",
    "placeholders": { "message": { "content": "$1" } }
  },
  "errorExportFailed": {
    "message": "내보내기 실패: $message$",
    "placeholders": { "message": { "content": "$1" } }
  },
  "errorImportFailed": {
    "message": "가져오기 실패: $message$",
    "placeholders": { "message": { "content": "$1" } }
  },
  "errorDownloadFailed": {
    "message": "다운로드 실패: $message$",
    "placeholders": { "message": { "content": "$1" } }
  }
